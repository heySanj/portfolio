<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">

  <title><%= projectTitle %> - <%= title %></title>
  <meta content="" name="description">
  <meta content="" name="keywords">

  <!-- Favicons -->
  <link href="../../assets/img/favicon.png" rel="icon">
  <link href="../../assets/img/apple-touch-icon.png" rel="apple-touch-icon">

  <!-- Google Fonts -->
  <link href="https://fonts.googleapis.com/css?family=Poppins:300,300i,400,400i,500,500i,600,600i,700,700i|Playfair+Display:400,400i,500,500i,600,600i,700,700i,900,900i" rel="stylesheet">

  <!-- FontAwesome -->
  <script src="https://kit.fontawesome.com/c19be33044.js" crossorigin="anonymous"></script>

  <!-- Vendor CSS Files -->
  <link href="../../assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  <link href="../../assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
  <link href="../../assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
  <link href="../../assets/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
  <link href="../../assets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

  <!-- Template Main CSS File -->
  <link href="../../assets/css/style.css" rel="stylesheet">

  <!-- Custom CSS -->
  <link href="../../assets/css/app.css" rel="stylesheet">

  <!-- =======================================================
  * Template Name: Folio - v4.9.1
  * Template URL: https://bootstrapmade.com/folio-bootstrap-portfolio-template/
  * Author: BootstrapMade.com
  * License: https://bootstrapmade.com/license/
  ======================================================== -->
</head>

<body>

  <!-- ======= Header ======= -->
  <header id="header" class="fixed-top header-inner-pages">
    <div class="container d-flex align-items-center justify-content-between">

    <!-- <a href="index.html" class="logo"><img src="assets/img/logo.png" alt="" class="img-fluid"></a> -->
    <!-- Uncomment below if you prefer to use an text logo -->
    <h1 class="logo"><a href="/">Sanj!</a></h1>

    <nav id="navbar" class="navbar">
    <ul>
        <li><a class="nav-link scrollto active" href="/#hero">Home</a></li>
        <li><a class="nav-link scrollto" href="/#about">About</a></li>
        <li><a class="nav-link  scrollto" href="/#portfolio">Portfolio</a></li>
        <li><a class="nav-link scrollto" href="/#contact">Contact</a></li>
    </ul>
    <i class="bi bi-list mobile-nav-toggle"></i>
    </nav><!-- .navbar -->

    </div>
  </header><!-- End Header -->

  <main id="main">
  
        <!-- ======= Portfolio Details Section ======= -->
        <section id="portfolio-details" class="portfolio-details">
          <div class="container">
    
            <div class="row gy-4">           
              
              <!-- Project Banner / Video element -->
              <div class="col-12 order-first">                
                <% if (banner) { %>                  
                    <img src="<%= banner %>" alt="" class="w-100">                  
                <% } else if (video) { %>
                  <video autoplay muted loop id="bannerVideo" class="w-100">
                    <source src="<%= video %>" type="video/mp4">
                  </video>               
                <% } %>
              </div>
    
              <div class="col-lg-8 order-last order-lg-first">
                <% for( let img of images ) { %>
                  <img src="<%= img %>" alt="" class="w-100 pb-4">
                <% } %>
              </div>
    
              <div class="col-lg-4 order-first order-lg-last">
                <div class="" style="top: 7em"> <!-- .sticky-top to keep it at the top -->
                  <div class="portfolio-info text-break">
                    <h3>Project information</h3>
                    <ul>
                      <% if (category) { %>
                        <li><strong>Category</strong>: <%= category %></li>
                      <% } %>
                      <% if (client) { %>
                        <li><strong>Client</strong>: <%= client %></li>
                      <% } %>
                      <% if (yr) { %>
                        <li><strong>Year</strong>: <%= yr %></li>
                      <% } %>
                      <% if (projectUrl) { %>
                        <li><strong>Project URL</strong>: <a href="<%= projectUrl %>"><%= projectUrl %></a></li>
                      <% } %>
                    </ul>

                    <% if (badges) { %>
                      <% for(let badge of badges) { %>
                        <span class="badge"><%= badge %></span>
                      <% } %>
                    <% } %>

                  </div>
                  <div class="portfolio-description px-2">
                    <%- content %>
                  </div>
                </div>

              </div>
    
            </div>
    
          </div>
        </section><!-- End Portfolio Details Section -->

  </main>

  <%- include('/partials/footer') %> 

  <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

  <!-- Vendor JS Files -->
  <script src="../../assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script src="../../assets/vendor/glightbox/js/glightbox.min.js"></script>
  <script src="../../assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
  <script src="../../assets/vendor/swiper/swiper-bundle.min.js"></script>
  <script src="../../assets/vendor/typed.js/typed.min.js"></script>
  <script src="../../assets/vendor/php-email-form/validate.js"></script>

  <!-- Template Main JS File -->
  <script src="../../assets/js/main.js"></script>

</body>

</html>